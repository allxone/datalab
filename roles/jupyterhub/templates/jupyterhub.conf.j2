# {{ ansible_managed }}

[program:jupyterhub]
command={{jupyterhub.srv_dir}}/start-jupyterhub.sh --config={{ jupyterhub.config_dir }}/{{ jupyterhub.config_file }}
redirect_stderr=true
stdout_logfile={{ jupyterhub.log_dir }}/jupyterhub.log
autostart=true
autorestart=false
stopasgroup=true
user={{ jupyterhub.user }}
directory={{jupyterhub.srv_dir}}
